generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       String @id
  password String
  email    String
  nick     String
  type     String
}

model Films {
  id          String @id
  title       String
  description String
  director    String
  scenario    String
  genre       String
  production  String
  premiere    String
  miniature   String
  content     String
  duration    String
  like        String
  dislike     String
}
